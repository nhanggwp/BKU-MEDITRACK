# Environment variables for MediTrack application
# Copy this file to .env and update with your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_role_key_here

# AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Security
SECRET_KEY=your_jwt_secret_key_here
QR_ENCRYPTION_KEY=your_qr_encryption_key_here

# Environment
ENVIRONMENT=production

# Redis Configuration (using Docker service)
REDIS_URL=redis://redis:6379

# Web App Configuration
REACT_APP_API_URL=http://localhost:8000
NODE_ENV=production

# Database Configuration (for DDI Service)
POSTGRES_DB=ddi_results
POSTGRES_USER=ddi_user
POSTGRES_PASSWORD=your_postgres_password_here

# Monitoring Configuration (for Grafana)
GF_SECURITY_ADMIN_PASSWORD=your_grafana_admin_password_here